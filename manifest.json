{
  "manifest_version": 3,
  "name": "Video Recorder Extension",
  "description": "It records the users'screen",
  "icons": {
    "128": "/images/icon-128x128.png"
  }
},

"background scripts":{
    "service_worker": "background.js"
},

"action": {
    "default_popup": "popup.html"
    "default_icon": {
        "16": "/images/icon-16x16.png",
        "48": "/images/icon-32x32.png",
        "128": "/images/icon-128x128.png"
    }
},

"host_permissions": [
    "http://*/*",
    "https://*/*",
],

"permissions": [
   
],

"version":"1.0.0",

